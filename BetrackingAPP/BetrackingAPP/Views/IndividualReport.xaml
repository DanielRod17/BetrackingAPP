<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xfx="clr-namespace:Xfx;assembly=Xfx.Controls"
    mc:Ignorable="d"
    x:Class="BetrackingAPP.Views.IndividualReport"
	x:Name="Page">
	<ContentPage Title="Detail" x:Name="Detail">
		<ScrollView>
			<Grid>
				<StackLayout x:Name="detailPanel" BackgroundColor="#f4f4f4">
					<StackLayout>
						<Label Text="DETAILS"
							FontSize="36"
							CharacterSpacing="2"
							TextColor="White"
							HeightRequest="90"
							HorizontalTextAlignment="Center"
							VerticalTextAlignment="Center"
							BackgroundColor="#15212f">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
					</StackLayout>
					<Grid Margin="15, 10, 15, 0" RowSpacing="0" BackgroundColor="#f4f4f4" ColumnSpacing="0">
						<Grid.RowDefinitions>
							<RowDefinition Height="45"/>
							<RowDefinition Height="45"/>
							<RowDefinition Height="{Binding DisplayReportMode}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="45"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="45"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="{Binding DisplayHeight}"/>
							<RowDefinition Height="45"/>
							<RowDefinition Height="45"/>
							<RowDefinition Height="50"/>
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
						</Grid.ColumnDefinitions>
						<!-- Inputs -->
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4" 
							Text="Assignment" 
							Grid.Row="0" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<!--<xfx:XfxComboBox Placeholder="Select an Assignment" Grid.Row="0" Grid.Column="3" Grid.ColumnSpan="4" 
							Text="{Binding AssignmentName}"
							ItemsSource="{Binding Assignments}"
							SortingAlgorithm="{Binding SortingAlgorithm}"
							HorizontalTextAlignment="Center"
							WidthRequest="300"/>-->
						<Picker Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="5"
							Title="Assignments"
							Margin="0, 0, 10, 0"
							ItemsSource="{Binding Assignments}"
							SelectedItem="{Binding AssignmentName}">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4" 
							Text="Name" 
							Grid.Row="1" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Entry Placeholder="Name" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="5" 
							Text="{Binding Name}">
							<Entry.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Entry.FontFamily>
						</Entry>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Type of Report" 
							Grid.Row="2" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="2"  Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedIndex="{Binding ReportType}" HorizontalOptions="FillAndExpand" Margin="0,0,10,0">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>Travel</x:String>
								<x:String>Expenses Only</x:String>
							</Picker.Items>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Type of Travel" 
							Grid.Row="3" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="3"  Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedIndex="{Binding Type_of_travel}" HorizontalOptions="FillAndExpand" Margin="0,0,10,0">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>Business Travel (BEPC Life Science)</x:String>
								<x:String>BE PRO (IT Division)</x:String>
								<x:String>Other Travel</x:String>
							</Picker.Items>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Flight" 
							Grid.Row="4" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedIndex="{Binding Flight}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>Required</x:String>
								<x:String>Not Required</x:String>
							</Picker.Items>
						</Picker>
						<!--  FROM -->
						<Label TextColor="#15212f" 
							BackgroundColor="#f4f4f4"
							FontAttributes="Bold"
							Text="FROM" 
							Grid.Row="5"
							CharacterSpacing="2"
							Grid.Column="0" 
							Grid.ColumnSpan="7" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Departure Date" 
							Grid.Row="6" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<DatePicker Grid.Row="6" Grid.Column="2" Grid.ColumnSpan="5" 
							MinimumDate="01/01/2019"
							Date ="{Binding FromDate}">
							<DatePicker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</DatePicker.FontFamily>
						</DatePicker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Departure Time" 
							Grid.Row="7" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="7" Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedIndex="{Binding From_Departure}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>Morning</x:String>
								<x:String>Afternoon</x:String>
								<x:String>Night</x:String>
							</Picker.Items>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Country" 
							Grid.Row="8" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="8" Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedItem="{Binding Country_From}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>MX</x:String>
								<x:String>US</x:String>
							</Picker.Items>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="State" 
							Grid.Row="9" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="9" Grid.Column="2" Grid.ColumnSpan="5" ItemsSource="{Binding StatesListFrom}"
							SelectedItem="{Binding State_From}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="City" 
							Grid.Row="10" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="10" Grid.Column="2" Grid.ColumnSpan="5"  ItemsSource="{Binding CitiesListFrom}"
							SelectedItem="{Binding City_From}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Date of Return" 
							Grid.Row="11" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<DatePicker Grid.Row="11" Grid.Column="2" Grid.ColumnSpan="5"  
							Date="{Binding ToDate}"
							MinimumDate="01/01/2019">
							<DatePicker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</DatePicker.FontFamily>
						</DatePicker>
						<!-- DESTINATION-->
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="DESTINATION" 
							CharacterSpacing="2"
							FontAttributes="Bold"
							Grid.Row="12" 
							Grid.Column="0" 
							Grid.ColumnSpan="7" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Date of Arrival" 
							Grid.Row="13" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<DatePicker Grid.Row="13" Grid.Column="2" Grid.ColumnSpan="5" 
							Date="{Binding Arrival_Date}"
							MinimumDate="01/01/2019"
							FontSize="Small">
							<DatePicker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</DatePicker.FontFamily>
						</DatePicker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Arrival Time" 
							Grid.Row="14" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="14" Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedIndex="{Binding To_Departure}" HorizontalOptions="FillAndExpand" >
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>Morning</x:String>
								<x:String>Afternoon</x:String>
								<x:String>Night</x:String>
							</Picker.Items>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Country" 
							Grid.Row="15" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="15" Grid.Column="2" Grid.ColumnSpan="5" 
							SelectedItem="{Binding Country_To}" HorizontalOptions="FillAndExpand" >
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
							<Picker.Items>
								<x:String>MX</x:String>
								<x:String>US</x:String>
							</Picker.Items>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="State" 
							Grid.Row="16" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="16" Grid.Column="2" Grid.ColumnSpan="5" ItemsSource="{Binding StatesListDest}"
							SelectedItem="{Binding State_To}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="City" 
							Grid.Row="17" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
						</Label>
						<Picker Grid.Row="17" Grid.Column="2" Grid.ColumnSpan="5"  ItemsSource="{Binding CitiesListDest}"
							SelectedItem="{Binding City_To}" HorizontalOptions="FillAndExpand">
							<Picker.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<!--<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>-->
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>
								</OnPlatform>
							</Picker.FontFamily>
						</Picker>
						<Label TextColor="Black" 
							BackgroundColor="#f4f4f4"
							Text="Seating Preference" 
							Grid.Row="18" 
							Grid.Column="0" 
							Grid.ColumnSpan="2" 
							HorizontalTextAlignment="Start"
							VerticalTextAlignment="Center"
							WidthRequest="40">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold"/>
									<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular"/>-->
								</OnPlatform>
							</Label.FontFamily>
