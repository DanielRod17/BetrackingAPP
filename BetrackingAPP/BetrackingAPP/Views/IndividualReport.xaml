				<StackLayout AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0, 0, 0, 0" BackgroundColor="Transparent" IsVisible="{Binding IsLoading}">
					<StackLayout VerticalOptions="CenterAndExpand">
						<ActivityIndicator IsRunning="True" Color="#f6d43b" HeightRequest="40" WidthRequest="40" Margin="0, -26, 0, 0" RotationX="180"/>
					</StackLayout>
				</StackLayout>
			</Grid>
		</ScrollView>
	</ContentPage>
	<ContentPage Title="Expenses">
		<ScrollView>
			<Grid>
				<StackLayout x:Name="ExpensesList" BackgroundColor="#F4F4F4">
					<Label Text="EXPENSES"
						CharacterSpacing="2"
						FontSize="30"
						TextColor="White"
						HeightRequest="90"
						HorizontalTextAlignment="Center"
						VerticalTextAlignment="Center"
						BackgroundColor="#15212f">
						<Label.FontFamily>
							<OnPlatform x:TypeArguments="x:String">
								<!--<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />-->
								<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
							</OnPlatform>
						</Label.FontFamily>
					</Label>
					<ListView
						x:Name="TimecardsList"
						ItemsSource="{Binding ExpensesList}"
						RowHeight="180"
						SeparatorVisibility="None"
						BackgroundColor="#f4f4f4">
						<ListView.ItemTemplate>
							<DataTemplate>
								<ViewCell >
									<Frame CornerRadius="5"
									Margin="10,2,10,0"
									BackgroundColor="White"
									HasShadow="False"
									BorderColor="#e4e4e4">
										<StackLayout>
											<Label Text="{Binding CategoryName}" FontSize="32">
												<Label.FontFamily>
													<OnPlatform x:TypeArguments="x:String">
														<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
													</OnPlatform>
												</Label.FontFamily>
											</Label>
											<Label Text="{Binding ExpenseDate, StringFormat='Date: {0:dd/MM/yyyy}'}" Margin="0, 5, 0, 0">
												<Label.FontFamily>
													<OnPlatform x:TypeArguments="x:String">
														<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
													</OnPlatform>
												</Label.FontFamily>
											</Label>
											<Label Text="{Binding ValorFinal, StringFormat='Quantity: ${0} USD'}"  Margin="0, 5, 0, 0">
												<Label.FontFamily>
													<OnPlatform x:TypeArguments="x:String">
														<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
													</OnPlatform>
												</Label.FontFamily>
											</Label>
											<Label IsVisible="{Binding MxOn}" Text="{Binding Quantity, StringFormat='Quantity: ${0} MXN'}"  Margin="0, 5, 0, 0">
												<Label.FontFamily>
													<OnPlatform x:TypeArguments="x:String">
														<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
													</OnPlatform>
												</Label.FontFamily>
											</Label>
											<Label IsVisible="{Binding DescriptionOn}" Text="{Binding Name, StringFormat='Description: {0}'}"  Margin="0, 5, 0, 0">
												<Label.FontFamily>
													<OnPlatform x:TypeArguments="x:String">
														<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
													</OnPlatform>
												</Label.FontFamily>
											</Label>
											<Label IsVisible="{Binding RefundableOn}" Text="Refundable" Margin="0, 5, 0, 0">
												<Label.FontFamily>
													<OnPlatform x:TypeArguments="x:String">
														<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
													</OnPlatform>
												</Label.FontFamily>
											</Label>
										</StackLayout>
									</Frame>
								</ViewCell>
							</DataTemplate>
						</ListView.ItemTemplate>
					</ListView>
					<Button
						Text="Add Expenses"
						BackgroundColor="#15212f"
						Clicked="AddExpenses"
						IsVisible="{Binding HasPropertyValueChanged}"
						TextColor="White"
						Margin="10, 0, 10, 10"
						FontSize="20">
						<Button.FontFamily>
							<OnPlatform x:TypeArguments="x:String">
								<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
							</OnPlatform>
						</Button.FontFamily>
					</Button>
				</StackLayout>
				<StackLayout AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0, 0, 1, 1" BackgroundColor="#15212f" Opacity="0.9" IsVisible="{Binding IsLoading}">
					<StackLayout VerticalOptions="CenterAndExpand">
						<ActivityIndicator IsRunning="True" Color="#f6d43b" HeightRequest="60" WidthRequest="60"/>
						<Label Text="Loading..." FontSize="19" TextColor="White" HorizontalTextAlignment="Center" VerticalOptions="Center">
							<Label.FontFamily>
								<OnPlatform x:TypeArguments="x:String">
									<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
								</OnPlatform>
							</Label.FontFamily>
						</Label>
					</StackLayout>
				</StackLayout>
				<StackLayout AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0, 0, 0, 0" BackgroundColor="Transparent" IsVisible="{Binding IsLoading}">
					<StackLayout VerticalOptions="CenterAndExpand">
						<ActivityIndicator IsRunning="True" Color="#f6d43b" HeightRequest="40" WidthRequest="40" Margin="0, -26, 0, 0" RotationX="180"/>
					</StackLayout>
				</StackLayout>
			</Grid>
		</ScrollView>
	</ContentPage>
	<ContentPage Title="Files">
		<Grid>
			<StackLayout x:Name="Files" BackgroundColor="#F4F4F4">
				<Label Text="FILES"
					CharacterSpacing="2"
					FontSize="30"
					TextColor="White"
					HeightRequest="90"
					HorizontalTextAlignment="Center"
					VerticalTextAlignment="Center"
					BackgroundColor="#15212f">
					<Label.FontFamily>
						<OnPlatform x:TypeArguments="x:String">
							<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
						</OnPlatform>
					</Label.FontFamily>
				</Label>
				<ListView
					x:Name="FilesList"
					ItemsSource="{Binding FilesList}"
					IsPullToRefreshEnabled="false"
					HasUnevenRows="true"
					SeparatorVisibility="None"
					BackgroundColor="#f4f4f4">
					<ListView.ItemTemplate>
						<DataTemplate>
							<ViewCell >
								<Frame CornerRadius="5"
									Margin="10,2,10,0"
									BackgroundColor="White"
									HasShadow="False"
									BorderColor="#e4e4e4">
									<StackLayout>
										<Label Text="{Binding Name}" FontSize="Large">
											<Label.FontFamily>
												<OnPlatform x:TypeArguments="x:String">
													<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
												</OnPlatform>
											</Label.FontFamily>
										</Label>
										<Label Text="{Binding Date, StringFormat='Date: {0}'}">
											<Label.FontFamily>
												<OnPlatform x:TypeArguments="x:String">
													<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
												</OnPlatform>
											</Label.FontFamily>
										</Label>
										<Label Text="{Binding Size, StringFormat='Size: {0}'}">
											<Label.FontFamily>
												<OnPlatform x:TypeArguments="x:String">
													<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
												</OnPlatform>
											</Label.FontFamily>
										</Label>
										<Button
											Command="{Binding Path=BindingContext.DownloadCommand, Source={x:Reference Page}}"
											CommandParameter="{Binding URL}"
											Text="Download" 
											TextColor="White"
											FontSize="18"
											BackgroundColor="#15212f"
											BorderRadius="5">
											<Button.FontFamily>
												<OnPlatform x:TypeArguments="x:String">
													<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
												</OnPlatform>
											</Button.FontFamily>
										</Button>
										<Button
											Command="{Binding Path=BindingContext.DeleteFileCommand, Source={x:Reference Page}}"
											CommandParameter="{Binding .}"
											Text="Delete" 
											TextColor="White"
											FontSize="18"
											BackgroundColor="#15212f"
											BorderRadius="5">
											<Button.FontFamily>
												<OnPlatform x:TypeArguments="x:String">
													<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
												</OnPlatform>
											</Button.FontFamily>
										</Button>
									</StackLayout>
								</Frame>
							</ViewCell>
						</DataTemplate>
					</ListView.ItemTemplate>
				</ListView>
				<Button
					Text="Add Files"
					BackgroundColor="#15212f"
					Clicked="AddFiles"
					IsVisible="{Binding HasPropertyValueChanged}"
					TextColor="White"
					Margin="10, 0, 10, 10"
					FontSize="20">
					<Button.FontFamily>
						<OnPlatform x:TypeArguments="x:String">
							<On Platform="Android" Value="BebasNeue Bold.ttf#BebasNeue Bold" />
						</OnPlatform>
					</Button.FontFamily>
				</Button>
			</StackLayout>
			<StackLayout AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0, 0, 1, 1" BackgroundColor="#15212f" Opacity="0.9" IsVisible="{Binding IsLoading}">
				<StackLayout VerticalOptions="CenterAndExpand">
					<ActivityIndicator IsRunning="True" Color="#f6d43b" HeightRequest="60" WidthRequest="60"/>
					<Label Text="Loading..." FontSize="19" TextColor="White" HorizontalTextAlignment="Center" VerticalOptions="Center">
						<Label.FontFamily>
							<OnPlatform x:TypeArguments="x:String">
								<On Platform="Android" Value="Questrial-Regular.ttf#Questrial-Regular" />
							</OnPlatform>
						</Label.FontFamily>
					</Label>
				</StackLayout>
			</StackLayout>
			<StackLayout AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0, 0, 0, 0" BackgroundColor="Transparent" IsVisible="{Binding IsLoading}">
				<StackLayout VerticalOptions="CenterAndExpand">
					<ActivityIndicator IsRunning="True" Color="#f6d43b" HeightRequest="40" WidthRequest="40" Margin="0, -26, 0, 0" RotationX="180"/>
				</StackLayout>
			</StackLayout>
		</Grid>
	</ContentPage>
</TabbedPage>